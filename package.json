    "commander": "^14.0.2",
    "dotenv": "^16.5.0",
    "gray-matter": "^4.0.3",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "openai": "^6.15.0",
    "tabtab": "^3.0.2",
    "twitter-api-v2": "^1.15.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/tabtab": "^3.0.5",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
