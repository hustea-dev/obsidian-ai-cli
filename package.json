    "test:e2e": "rm -rf dist_tests && tsc -p tsconfig.e2e.json && cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --testMatch '**/tests/e2e/**/*.e2e.test.ts'"
  },
  "dependencies": {
